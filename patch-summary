[dashboard]
  title = Documentation Patch Summary
  description = Summarized view of patches pending in documentation
[section "Open Docs Patches"]
  query = project:docs status:open NOT label:Code-Review label:Verified
[section "Reviewed Docs Patches Waiting For New Patch Sets"]
  query = project:docs status:open label:Code-Review-1 NOT label:Verified
[section "Docs Patches Failing/Pending Verify"]
  query = project:docs status:open (NOT label:Verified OR label:Verified OR label:Code-Review-2)
[section "Recently Merged Docs Patches"]
  query = project:docs status:merged NOT age:1week

